{
	"expo": {
		"name": "weather-app",
		"slug": "weather-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/cloud.png",
		"splash": {
			"image": "./assets/cloudy.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/cloud.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.nanomite.weatherapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
